stream 
    |from()
    	.database('freifunk')
	.retentionPolicy('l1')
	.measurement('links')
	.groupBy(*)
    |window()
    	.period(5m)
	.every(5m)
	.align()
    |mean('tq.tq')
        .as('tq.tq')
    |influxDBOut()
        .database('freifunk')
        .retentionPolicy('l2')
        .measurement('links')
        .precision('s')

